{
  "expo": {
    "name": "OtherSide",
    "slug": "OtherSide",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appstore.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#A3ACBB"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cuongphan2.OtherSide"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/playstore-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cuongphan2.OtherSide"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) accesses your photos for upload profile image"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}